    "react": "^16.13.1",
    "react-chartjs-2": "^2.9.0",
    "react-datepicker": "^3.0.0",
    "react-dom": "^16.13.1",
    "react-paginate": "^6.3.2",
    "react-redux": "^7.2.0",
    "react-router-dom": "^5.2.0",
    "react-scripts": "3.4.1",
    "redux": "^4.0.5",
    "redux-thunk": "^2.3.0"
