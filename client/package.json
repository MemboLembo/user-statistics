    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-prettier": "^3.1.3",
    "eslint-plugin-react": "^7.20.0",
    "eslint-plugin-react-redux": "^3.0.3",
    "lint-staged": "^10.2.6",
    "node-sass": "^7.0.0",
    "prettier": "^2.0.5"
  },
  "proxy": "http://localhost:4000"
